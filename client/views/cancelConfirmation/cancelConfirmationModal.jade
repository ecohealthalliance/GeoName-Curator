template(name="cancelConfirmationModal")
  .modal.fade#cancelConfirmationModal(role="dialog")
    .modal-dialog
      .modal-content
        form
          .close-modal(data-dismiss="modal" aria-label="Close")
          .modal-header
            .modal-header--title
              h4 Confirm Cancel
          .modal-body.clearfix
            +cancelConfirmationModalBody data
