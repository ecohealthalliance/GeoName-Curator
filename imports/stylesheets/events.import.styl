.events
  table
  thead
  tbody
  th
  td
  tr
    +below(3)
      display block

  thead tr
    +below(3)
      position absolute
      top -9999px
      left -9999px

    tr
      +below(3)
        border 0

    td
      +below(3)
        border none
        border-bottom 1px solid #eee
        position relative
        padding-left 50%

    .table-striped > tbody > tr:nth-child(odd) > td
    .table-striped > tbody > tr:nth-child(odd) > th
      +below(3)
        background none

.dropdown-toggle-nav
  font-size 0
  &::before,
  &::after
    vAlign()

.dropdown-menu > .active > a
.dropdown-menu > .active > a:hover
.dropdown-menu > .active
  background-color $secondary

.dropdown-menu
  max-width 300px
.dropdown-menu-right
  left 0 !important
  right 0
.dropdown-menu.nav-dd
  padding 0

#proMedIFrame
  width 100%
  height 600px
